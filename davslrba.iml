class Fraction{
    var numerator: Double = 0.0
    var denominator: Double = 0.0
    var mricxveli: Double = 0.0
    var mnishvneli: Double = 0.0
    fun mimateba(Fraction: Any): Double{
        var Nikoloz = (numerator/denominator) + (mricxveli/mnishvneli)
        return Nikoloz

    }
    fun gamravleba(Fraction: Any): Double{
        var Kacitadze = (numerator*mricxveli) / (denuminator*mnishvneli)
        return kacitadze

    }







